{
  "validateIndentation": 4, // I changed this to 4 from 2 to fit Visual Studio's default indentation
  "requireSpaceAfterKeywords": [
    "if",
    "else",
    "for",
    "while",
    "do",
    "switch",
    "return",
    "try",
    "catch"
  ],
  "maximumLineLength": 80,
  "validateQuoteMarks": "'",
  "disallowTrailingWhitespace": true
}
